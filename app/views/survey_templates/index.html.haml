%h1 SurveyOski Evaluation

%table#forms
  %thead
    %tr
      %th= "Forms"
  %tbody
    - @templates.each do |template|
      %tr
        - if template.survey_title.to_s != ''
          %td= link_to template.survey_title.to_s, survey_template_path(template)
        - else
          %td= link_to "Survey " + template.id.to_s, survey_template_path(template)

