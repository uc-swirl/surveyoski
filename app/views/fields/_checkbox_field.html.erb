<%= label_tag "submission[" + field.id.to_s + "]", field.question_title, class: "field_label", id: field.id %>
<% field.field_options.each do |option| %>
	<br>
	<label for=<%="submission_#{field.id}_#{option[0]}"%>><%=option[0]%>
	<%= check_box_tag("submission[" + field.id.to_s + "][]", option[1], false, :id => "submission_#{field.id}_#{option[0]}") %>
	</label>
    
<% end %>